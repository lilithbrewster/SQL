-- Query results

SELECT name, year, status, gender, space_flights FROM astronauts;


--Calculate resultes using CASE and COUNT(*)

SELECT COUNT(*),
    CASE
        WHEN space_flights > 5 THEN'above_target'
        WHEN space_flights > 2 THEN 'target'
        WHEN space_flights > 1 THEN 'below_target'
    ELSE 'F'
END as "completed_flights" FROM astronauts
GROUP BY "completed_flights";


-- Display use of SUM

SELECT year, SUM(Space_walks_hr)
AS sum_hrs FROM astronauts
GROUP BY gender
HAVING sum_hrs > 150;


--Display use of complex queries of AND/OR

SELECT name, year
FROM astronauts
WHERE groupnum > "17"
AND year > 1995
ORDER BY year desc
LIMIT 10;





