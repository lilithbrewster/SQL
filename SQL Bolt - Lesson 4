-- List all the directors of Pixar movies (alphabetically) without duplicates
SELECT distinct director
FROM movies
ORDER BY director ASC;

-- List the last four Pixar movies released (ordered by most recent to last)
SELECT title, year
FROM movies
ORDER BY year DESC
LIMIT 4;

-- List the first five Pixar movies sorted alphabetically 
SELECT title
FROM movies
ORDER BY title ASC
LIMIT 5;

--List the next 5 Pixar movies sorted alphabetically 
SELECT title
FROM movies
ORDER BY title ASC
LIMIT 5;
